{
  "name": "@hrcd/eslint-config",
  "version": "2.0.4",
  "description": "My personal eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/eslint-config.git",
    "directory": "packages/eslint-config"
  },
  "funding": "https://github.com/sponsors/HugoRCD",
  "author": "HugoRCD",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "automd": "automd",
    "build": "unbuild",
    "dev": "unbuild && eslint .",
    "lint": "eslint .",
    "lint:fix": "eslint . --cache --fix",
    "release": "bun run build && changelogen --release --push",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.10.0",
    "@nuxt/eslint-plugin": "^0.5.6",
    "@rushstack/eslint-patch": "^1.10.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-package-json": "^0.15.3",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-vue": "^9.28.0",
    "jsonc-eslint-parser": "^2.4.0",
    "tslib": "^2.7.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.9",
    "@types/eslint": "^9.6.1",
    "automd": "^0.3.8",
    "bun-plugin-dts": "^0.2.3",
    "changelogen": "^0.5.5",
    "eslint": "^9.10.0",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "packageManager": "bun@1.0.30"
}
