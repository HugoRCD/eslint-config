{
  "name": "@hrcd/eslint-config",
  "version": "1.0.0",
  "author": "HugoRCD",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/eslint-config.git"
  },
  "description": "My personal eslint config",
  "funding": "https://github.com/sponsors/HugoRCD",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "automd": "automd",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --cache --fix",
    "release": "changelogen --release --push",
    "build": "unbuild",
    "dev": "unbuild && eslint ."
  },
  "peerDependencies": {
    "eslint": "^9.1.1"
  },
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "@nuxt/eslint-plugin": "^0.3.10",
    "@rushstack/eslint-patch": "^1.10.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-flat-config-utils": "^0.2.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.25.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.1.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.2"
  },
  "packageManager": "bun@1.0.30"
}
